MINECRAFT_SAVE_DIR_19=
MINECRAFT_SAVE_DIR_20=

# ウィジェットのデータ収集の実行間隔
WIDGET_INTERVAL=2
# ワールド名のプレフィックス
WORLD_NAME_PREFIX='Random Speedrun #'